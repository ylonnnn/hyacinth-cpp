// Forward Declaration
struct Vector2;
struct Vector3; 

fn get_xy -> Vector2 (vec3: Vector3);

struct Player {
    name: str;
    rotation: Vector2;
    position: Vector3;
}

struct Vector2 {
    x: int[32];
    y: int[32];
}

struct Vector3 {
    x: int[32];
    y: int[32];
    z: int[32];
}

fn create_rotation -> Vector2 (x: int[32], y: int[32]) {
    return {
        x: x, y: y
    };
}

fn create_position -> Vector3 (x: int[32], y: int[32], z: int[32]) {
    return {
        x: x,
        y: y,
        z: z,
    };
}

