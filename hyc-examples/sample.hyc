import "./test-math.hyc";

struct Person {
    name: str;
    age: int[32];
}

fn use -> Person (p: Person) {
    return Person { name: "test", age: 12 };
}

fn main -> void () {
    let p: Person = Person { name: "John Doe", age: 32, };
    let p2: Person = use(p);

    let p3: Person = use(Person {name: "Non Bonjour", age: 21});

    let p4: Person = use(
        use({
            name: "Test",
            age: 23,
            is_employed: false,
        })
    );
}
